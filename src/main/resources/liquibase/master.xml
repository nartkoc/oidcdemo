<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:pro="http://www.liquibase.org/xml/ns/pro"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
		http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">

	<property name="uuidGenerate" value="public.uuid_generate_v4()" dbms="postgresql" />

	<include file="liquibase/language.xml" relativeToChangelogFile="false" />
	<include file="liquibase/currency.xml" relativeToChangelogFile="false" />
	<include file="liquibase/constraints.xml" relativeToChangelogFile="false" />
</databaseChangeLog>
